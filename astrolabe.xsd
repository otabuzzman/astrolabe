<xs:schema xmlns="http://www.chartaecaeli.eu/astrolabe/model" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.chartaecaeli.eu/astrolabe/model" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="AstrolabeType">
		<xs:sequence>
			<xs:element ref="Epoch"/>
			<xs:element ref="Chart" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="locale" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="([a-z][a-z])|([a-z][a-z]_[A-Z][A-Z])"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Astrolabe" type="AstrolabeType"/>
	<xs:element name="Epoch">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DateType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ChartAzimuthalType">
		<xs:sequence>
			<xs:element ref="Origin"/>
			<xs:element ref="Horizon" maxOccurs="unbounded"/>
			<xs:element ref="Atlas" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="pagesize" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="a[0-5]|[0-9]+(\.[0-9]+)?x[0-9]+(\.[0-9]+)?"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="viewport" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:maxInclusive value="100"/>
					<xs:minExclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="layout" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[a-zA-Z]+|(((0|([1-9][0-9]*))(\.[0-9]*[1-9])?(:(0|([1-9][0-9]*))(\.[0-9]*[1-9])?)*)x((0|([1-9][0-9]*))(\.[0-9]*[1-9])?(:(0|([1-9][0-9]*))(\.[0-9]*[1-9])?)*))"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="scale" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minExclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="hemisphere" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="northern|southern"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Chart">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="ChartStereographic"/>
				<xs:element ref="ChartOrthographic"/>
				<xs:element ref="ChartEquidistant"/>
				<xs:element ref="ChartGnomonic"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Origin" type="SphericalType"/>
	<xs:element name="Extent" type="SphericalType"/>
	<xs:element name="ChartStereographic" type="ChartAzimuthalType"/>
	<xs:element name="ChartGnomonic" type="ChartAzimuthalType"/>
	<xs:element name="ChartOrthographic" type="ChartAzimuthalType"/>
	<xs:element name="ChartEquidistant" type="ChartAzimuthalType"/>
	<xs:complexType name="AtlasType">
		<xs:sequence>
			<xs:element ref="Origin"/>
			<xs:element ref="Extent"/>
			<xs:choice>
				<xs:element name="SpanDe" type="AngleType"/>
				<xs:element name="SpanRA" type="AngleType"/>
			</xs:choice>
			<xs:element ref="Catalog" maxOccurs="unbounded"/>
			<xs:element ref="AtlasPage" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="pagesizex" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minInclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="pagesizey" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minInclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="checker" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="(([1-9]|([1-9][0-9]+))x([2-9]|([1-9][0-9]+)))|(([2-9]|([1-9][0-9]+))x([1-9]|([1-9][0-9]+)))"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="marshal" type="xs:anyURI" use="optional"/>
	</xs:complexType>
	<xs:element name="Atlas" type="AtlasType"/>
	<xs:complexType name="AtlasPageType">
		<xs:sequence>
			<xs:element name="P0" type="SphericalType"/>
			<xs:element name="P1" type="SphericalType"/>
			<xs:element name="P2" type="SphericalType"/>
			<xs:element name="P3" type="SphericalType"/>
			<xs:element ref="Origin"/>
			<xs:element name="Top" type="SphericalType"/>
			<xs:element name="Bottom" type="SphericalType"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="scale" type="xs:double" use="optional"/>
		<xs:attribute name="p0x" type="xs:double" use="optional"/>
		<xs:attribute name="p0y" type="xs:double" use="optional"/>
		<xs:attribute name="p1x" type="xs:double" use="optional"/>
		<xs:attribute name="p1y" type="xs:double" use="optional"/>
		<xs:attribute name="p2x" type="xs:double" use="optional"/>
		<xs:attribute name="p2y" type="xs:double" use="optional"/>
		<xs:attribute name="p3x" type="xs:double" use="optional"/>
		<xs:attribute name="p3y" type="xs:double" use="optional"/>
		<xs:attribute name="originx" type="xs:double" use="optional"/>
		<xs:attribute name="originy" type="xs:double" use="optional"/>
		<xs:attribute name="topx" type="xs:double" use="optional"/>
		<xs:attribute name="topy" type="xs:double" use="optional"/>
		<xs:attribute name="bottomx" type="xs:double" use="optional"/>
		<xs:attribute name="bottomy" type="xs:double" use="optional"/>
		<xs:attribute name="tcp" type="xs:positiveInteger" use="optional"/>
		<xs:attribute name="bcp" type="xs:positiveInteger" use="optional"/>
		<xs:attribute name="pcp" type="xs:positiveInteger" use="optional"/>
		<xs:attribute name="fcp" type="xs:positiveInteger" use="optional"/>
	</xs:complexType>
	<xs:element name="AtlasPage">
		<xs:annotation>
			<xs:documentation>Automatisch generiert falls Atlas-Element in ChartType vorhanden ist</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="AtlasPageType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="HorizonType">
		<xs:sequence>
			<xs:element ref="Latitude"/>
			<xs:element ref="Longitude" minOccurs="0"/>
			<xs:element ref="Date" minOccurs="0"/>
			<xs:element ref="Circle" maxOccurs="unbounded"/>
			<xs:element ref="Body" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Catalog" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="practicality" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="none|never|seldom|sometimes|frequent|always"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Horizon">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="HorizonLocal"/>
				<xs:element ref="HorizonEquatorial"/>
				<xs:element ref="HorizonEcliptical"/>
				<xs:element ref="HorizonGalactic"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="HorizonEquatorial" type="HorizonType"/>
	<xs:element name="HorizonLocal">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="HorizonType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HorizonEcliptical">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="HorizonType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="HorizonGalactic" type="HorizonType"/>
	<xs:complexType name="CircleType">
		<xs:sequence>
			<xs:element ref="Angle"/>
			<xs:element ref="Begin"/>
			<xs:element ref="End"/>
			<xs:element ref="Dial" minOccurs="0"/>
			<xs:element ref="Annotation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="fov" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="importance" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="canonical|cardinal"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Circle">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="CircleParallel"/>
				<xs:element ref="CircleMeridian"/>
				<xs:element ref="CircleSouthernPolar"/>
				<xs:element ref="CircleNorthernPolar"/>
				<xs:element ref="CircleSouthernTropic"/>
				<xs:element ref="CircleNorthernTropic"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="MarkType">
		<xs:choice>
			<xs:element ref="Immediate"/>
			<xs:element ref="Reference"/>
		</xs:choice>
	</xs:complexType>
	<xs:element name="Begin" type="MarkType"/>
	<xs:element name="End" type="MarkType"/>
	<xs:complexType name="ReferenceType">
		<xs:attribute name="circle" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[^,]+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="node" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="chasing|leading"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Reference">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ReferenceType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Immediate" type="AngleType"/>
	<xs:element name="CircleParallel" type="CircleType"/>
	<xs:element name="CircleMeridian" type="CircleType"/>
	<xs:element name="CircleSouthernPolar">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CircleType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CircleNorthernPolar">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CircleType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CircleSouthernTropic">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CircleType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="CircleNorthernTropic">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="CircleType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="DialType">
		<xs:sequence>
			<xs:element ref="GraduationSpan"/>
			<xs:element ref="GraduationHalf" minOccurs="0"/>
			<xs:element ref="GraduationFull" minOccurs="0"/>
			<xs:element ref="Annotation" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="baseline" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="none|line|rail"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="reflect" type="xs:boolean" use="required"/>
	</xs:complexType>
	<xs:element name="Dial">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="DialDegree"/>
				<xs:element ref="DialHour"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="DialDegree">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DialType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DialHour" type="DialType"/>
	<xs:element name="DialDay">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="DialType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="GraduationType">
		<xs:sequence>
			<xs:element ref="AnnotationStraight" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="span" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minExclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="GraduationSpan">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="GraduationType">
					<xs:attribute name="division" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:positiveInteger">
								<xs:minInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="GraduationHalf">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="GraduationType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="GraduationFull">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="GraduationType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="AnnotationType">
		<xs:sequence>
			<xs:element ref="Text" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="anchor" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="topleft|topmiddle|topright|middleleft|middle|middleright|bottomleft|bottommiddle|bottomright"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="reverse" type="xs:boolean" use="required"/>
	</xs:complexType>
	<xs:element name="Annotation">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="AnnotationStraight"/>
				<xs:element ref="AnnotationCurved"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="AnnotationStraight">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="AnnotationType">
					<xs:sequence>
						<xs:element name="Frame" minOccurs="0">
							<xs:complexType>
								<xs:attribute name="anchor" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="topleft|topmiddle|topright|middleleft|middle|middleright|bottomleft|bottommiddle|bottomright"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="number" type="xs:positiveInteger" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="radiant" type="xs:boolean" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AnnotationCurved">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="AnnotationType">
					<xs:attribute name="distance" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:double">
								<xs:minExclusive value="0"/>
								<xs:maxInclusive value="100"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="TextType">
		<xs:sequence>
			<xs:element ref="Subscript" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Superscript" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="value" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:element name="Text">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="TextType">
					<xs:attribute name="purpose" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="bodylettering[07]|titlelettering|textlettering|circlelettering|diallettering|dialcardinal|dialfraction|guidance|obscure|invisible|zenit|(mag([x]|(-?[0-9]+)))"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Subscript" type="TextType"/>
	<xs:element name="Superscript" type="TextType"/>
	<xs:element name="Body">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="BodyStellar"/>
				<xs:element ref="BodyAreal"/>
				<xs:element ref="BodyPlanet"/>
				<xs:element ref="BodyMoon"/>
				<xs:element ref="BodySun"/>
				<xs:element ref="BodyElliptical"/>
				<xs:element ref="BodyParabolical"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="BodyStellarType">
		<xs:sequence>
			<xs:element ref="Annotation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Text"/>
			<xs:element ref="Position"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="turn" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minInclusive value="0"/>
					<xs:maxExclusive value="24"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="spin" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:maxInclusive value="0"/>
					<xs:minExclusive value="-24"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="BodyStellar" type="BodyStellarType"/>
	<xs:element name="Position">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="SphericalType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="BodyArealType">
		<xs:sequence>
			<xs:element ref="Annotation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Position" minOccurs="2" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="area|sign"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="BodyAreal">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BodyArealType"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="BodyOrbitalType">
		<xs:sequence>
			<xs:element ref="Annotation" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Epoch" minOccurs="0"/>
			<xs:element ref="DialDay" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="stretch" type="xs:boolean" use="optional" default="false"/>
	</xs:complexType>
	<xs:element name="BodyPlanet">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BodyOrbitalType">
					<xs:attribute name="type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="mercury|venus|mars|jupiter|saturn|uranus|neptune"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BodyMoon" type="BodyOrbitalType"/>
	<xs:element name="BodySun">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BodyOrbitalType">
					<xs:attribute name="type" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="mean|true"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="circle" use="optional">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="[^,]+"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="BodyElliptical">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="BodyOrbitalType">
					<xs:attribute name="a" type="xs:double" use="required"/>
					<xs:attribute name="e" type="xs:double" use="required"/>
