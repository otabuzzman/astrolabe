		<xs:attribute name="bcp" type="xs:positiveInteger" use="required"/>
		<xs:attribute name="fcp" type="xs:positiveInteger" use="required"/>
		<xs:attribute name="pcp" type="xs:positiveInteger" use="required"/>
		<xs:attribute name="p0x" type="xs:double" use="required"/>
		<xs:attribute name="p0y" type="xs:double" use="required"/>
		<xs:attribute name="p1x" type="xs:double" use="required"/>
		<xs:attribute name="p1y" type="xs:double" use="required"/>
		<xs:attribute name="p2x" type="xs:double" use="required"/>
		<xs:attribute name="p2y" type="xs:double" use="required"/>
		<xs:attribute name="p3x" type="xs:double" use="required"/>
		<xs:attribute name="p3y" type="xs:double" use="required"/>
		<xs:attribute name="centerx" type="xs:double" use="required"/>
		<xs:attribute name="centery" type="xs:double" use="required"/>
		<xs:attribute name="topx" type="xs:double" use="required"/>
		<xs:attribute name="topy" type="xs:double" use="required"/>
		<xs:attribute name="bottomx" type="xs:double" use="required"/>
		<xs:attribute name="bottomy" type="xs:double" use="required"/>
