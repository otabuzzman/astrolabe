		<xs:attribute name="name" type="xs:string" use="optional"/>
		<xs:attribute name="value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:double">
					<xs:minExclusive value="0"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Scaleline">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ScalelineType"/>
